@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}

<div class="row justify-content-center">
    <div class="col-md-12 mt-2 widget widget-one_hybrid widget-followers p-5 mt-2">
        <h2 class="newTicket">Manage your account</h2>

        <div>
            <h4 class="newTicket">Change your account settings</h4>
            <hr />
            <div class="row newTicket">
                <div class="col-md-3">
                   <p class="newTicket"> <partial name="_ManageNav" /></p>
                </div>
                <div class="col-md-9">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
