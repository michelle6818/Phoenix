@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Reset password confirmation";
}

<div class="row justify-content-center">
    <div class="col-md-12 mt-2 widget widget-one_hybrid widget-followers p-5 mt-2">
        <h1 class="newTicket">@ViewData["Title"]</h1>
        <p class="newTicket">
            Your password has been reset. Please <a asp-page="./Login">click here to log in</a>.
        </p>
        </div>
</div>