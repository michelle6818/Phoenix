@model Phoenix.Models.Invite

@{
    ViewData["Title"] = "Details";
}



<div class="row justify-content-center">
    <div class="col-md-6 mt-2 widget widget-one_hybrid widget-followers p-5 mt-2">
        <h4>Invite Details</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CompanyToken)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CompanyToken)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.InviteDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.InviteDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IsValid)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.IsValid)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Company)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Company.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Invitor)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Invitor.FullName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Invitee)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Invitee.FullName)
            </dd>
        </dl>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
        <a asp-action="Index">Back to List</a>
